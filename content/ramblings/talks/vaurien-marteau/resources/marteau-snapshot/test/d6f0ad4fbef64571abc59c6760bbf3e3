<script type="text/javascript">
$(document).ready(function() {
  // connect to the websocket
  var socket = io.connect();

  socket.emit("subscribe", 'console.d6f0ad4fbef64571abc59c6760bbf3e3');

  socket.on("console.d6f0ad4fbef64571abc59c6760bbf3e3", function(line) {

    if (line.startsWith("END.")) {

      var status = line.split("END.")
      status = status[1];
      $('#status').text(status);

      if (status == "Success") {
       $("#actions").prepend('<a id="report" class="label" href="http://ec2-54-245-158-26.us-west-2.compute.amazonaws.com/report/d6f0ad4fbef64571abc59c6760bbf3e3/index.html">Report</a>\n');
      }

     if (!$('#delete').length) { 
         $("#actions").prepend('<a id="delete" class="label" href="http://ec2-54-245-158-26.us-west-2.compute.amazonaws.com/test/d6f0ad4fbef64571abc59c6760bbf3e3/delete">Delete</a>\n');
     }

     if (!$('#replay').length) { 
       $("#actions").prepend('<a id="replay" class="label" href="http://ec2-54-245-158-26.us-west-2.compute.amazonaws.com/test/d6f0ad4fbef64571abc59c6760bbf3e3/replay">Replay</a>\n');
     }
    }
    else {
      $('#console').append(line);
      $("#console").scrollTop($("#console")[0].scrollHeight);
   }
  });



});
</script>

