  <script type="text/javascript">$(function() {
    $('#signin').click(function() { navigator.id.request({"siteName": "Marteau"}); return false;});

    $('#signout').click(function() { navigator.id.logout(); return false;});

    var currentUser = null;

    navigator.id.watch({
        loggedInUser: currentUser,
        onlogin: function(assertion) {
            if (assertion) {
                var $form = $("<form method=POST "+
                    "      action='http://ec2-54-245-158-26.us-west-2.compute.amazonaws.com/login'>" +
                    "  <input type='hidden' " +
                    "         name='assertion' " +
                    "         value='" + assertion + "' />" +
                    "  <input type='hidden' " +
                    "         name='came_from' "+
                    "         value='http://ec2-54-245-158-26.us-west-2.compute.amazonaws.com/test/d6f0ad4fbef64571abc59c6760bbf3e3' />" +
                    "  <input type='hidden' " +
                    "         name='csrf_token' "+
                    "         value='20854977ed1caa29cb20ebcab498210013160b21' />" +
                    "</form>").appendTo($("body"));
                $form.submit();
            }
        },
        onlogout: function() {
            var $form = $("<form method=POST "+
                "      action='http://ec2-54-245-158-26.us-west-2.compute.amazonaws.com/logout'>" +
                "  <input type='hidden' " +
                "         name='came_from' "+
                "         value='http://ec2-54-245-158-26.us-west-2.compute.amazonaws.com/test/d6f0ad4fbef64571abc59c6760bbf3e3' />" +
                "  <input type='hidden' " +
                "         name='csrf_token' "+
                "         value='20854977ed1caa29cb20ebcab498210013160b21' />" +
                "</form>").appendTo($("body"));
            $form.submit();
        }
    });
});
</script>
